package com.mph.model;

public class Employee {
	// private int empno =101;
	// private String ename ="Ashwin";
	private int empno;
	private String ename;

	/*
	 * public void showDetails(){ System.out.println(empno + " " + ename); }
	 */
	public Employee(int empno, String ename) {
		System.out.println("from emp constuctor...........");
		this.empno = empno;
		this.ename = ename;
	}

	public int getEmpno() {
		return empno;
	}

	public void setEmpno(int empno) {
		this.empno = empno;
	}

	public String getEname() {
		return ename;
	}

	public void setEname(String ename) {
		this.ename = ename;

	}

	public String toString() {
		return "Employee [empno=" + empno + ", ename=" + ename + "]";
	}

}
